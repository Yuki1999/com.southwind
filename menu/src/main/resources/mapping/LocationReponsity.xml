<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.southwind.repository.LocationRepository">

    <select id="findById" parameterType="long" resultType="Location">
		select * from t_location where id = #{id}
	</select>

    <select id="findAll" resultType="Location">
		select * from t_location
	</select>

</mapper>